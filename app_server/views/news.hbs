<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>News - Travlr Getaways Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div id="news" class="body">
							<div class="sidebar">
			{{#each sideBar}}
								<h3>{{{this.name}}}</h3>
								<ul>
									<li>{{{this.headlines}}}</li>
			{{/each}}
								</ul>
							</div>
							<div>
			{{#each newsArticles}}
								<h1>{{{this.name}}}</h1>
								<a href="/news"><img src="images/{{this.image}}" alt="Img"></a>
								{{{this.description}}}
			{{/each}}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>